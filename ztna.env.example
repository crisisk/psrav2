# ZTNA Implementatie Environment Variables
# Kopieer dit bestand naar .env en vul de juiste waarden in

# Docker Registry
DOCKER_REGISTRY=registry.sevensa.nl

# Let's Encrypt
ACME_EMAIL=admin@sevensa.nl

# Keycloak
KEYCLOAK_ADMIN_PASSWORD=changeme
KEYCLOAK_DB_USER=keycloak
KEYCLOAK_DB_PASSWORD=changeme

# Traefik Forward Auth
TRAEFIK_FORWARD_AUTH_SECRET=changeme
TRAEFIK_FORWARD_AUTH_COOKIE_SECRET=changeme

# RentGuy
RENTGUY_DB_PASSWORD=changeme
RENTGUY_REDIS_PASSWORD=changeme
RENTGUY_JWT_SECRET=changeme
RENTGUY_CLIENT_SECRET=changeme

# PSRA
PSRA_DB_PASSWORD=changeme
PSRA_REDIS_PASSWORD=changeme
PSRA_JWT_SECRET=changeme
PSRA_CLIENT_SECRET=changeme

# WPCS
WPCS_DB_PASSWORD=changeme
WPCS_CLIENT_SECRET=changeme

# AI Orchestrator
AI_ORCHESTRATOR_CLIENT_SECRET=changeme

# SMTP
SMTP_HOST=smtp.sevensa.nl
SMTP_PORT=587
SMTP_USER=no-reply@sevensa.nl
SMTP_PASSWORD=changeme
SMTP_FROM=no-reply@sevensa.nl
